    "eslint-config-prettier": "8.1.0",
    "fs-extra": "^9.0.1",
    "husky": "~7.0.1",
    "jest": "27.2.3",
    "karma": "6.3.3",
    "karma-jasmine": "4.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-nativescript-launcher": "0.4.0",
    "karma-sinon": "^1.0.5",
    "karma-webpack": "5.0.0",
    "lint-staged": "^11.0.0",
    "ng-packagr": "13.0.8",
    "postcss": "^8.3.9",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^10.1.1",
