    "@nrwl/tao": "13.3.12",
    "@nrwl/workspace": "13.3.12",
    "@nstudio/angular": "13.0.1",
    "@nstudio/nativescript": "13.0.1",
    "@nstudio/nativescript-angular": "13.0.1",
    "@nstudio/xplat": "13.0.1",
    "@types/jasmine": "^3.7.2",
    "@types/jest": "27.0.2",
    "@types/node": "14.14.33",
